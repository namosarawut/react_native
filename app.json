{
  "expo": {
    "name": "Merasulay",
    "slug": "merasulay",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic.png",
    "splash": {
      "image": "./assets/intro.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.namoprojectapp.merasulay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
